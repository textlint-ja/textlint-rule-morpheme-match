{
  "name": "@textlint-ja/textlint-rule-morpheme-match",
  "version": "1.0.2",
  "description": "形態素解析結果のTokenベースの辞書でマッチするtextlintルール",
  "keywords": [
    "textlintrule"
  ],
  "homepage": "https://github.com/textlint-ja/textlint-rule-morpheme-match",
  "bugs": {
    "url": "https://github.com/textlint-ja/textlint-rule-morpheme-match/issues"
  },
  "license": "MIT",
  "author": "azu",
  "files": [
    "bin/",
    "lib/",
    "src/"
  ],
  "main": "lib/textlint-rule-morpheme-match.js",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/textlint-ja/textlint-rule-morpheme-match.git"
  },
  "scripts": {
    "build": "textlint-scripts build",
    "prepublish": "npm run --if-present build",
    "test": "textlint-scripts test",
    "prettier": "prettier --write \"**/*.{js,jsx,ts,tsx,css}\"",
    "watch": "textlint-scripts build --watch"
  },
  "prettier": {
    "printWidth": 120,
    "singleQuote": false,
    "tabWidth": 4
  },
  "devDependencies": {
    "husky": "^1.2.0",
    "lint-staged": "^8.1.0",
    "prettier": "^1.15.3",
    "textlint-scripts": "^2.1.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "husky": {
    "hooks": {
      "precommit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,css}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "kuromojin": "^1.4.0",
    "morpheme-match-all": "^1.1.0",
    "untildify": "^3.0.3"
  }
}
